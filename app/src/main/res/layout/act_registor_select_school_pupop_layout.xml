    card_view:cardUseCompatPadding="true">

    <ListView
        android:id="@+id/lv_school"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="@color/md_grey_300"
        android:listSelector="@drawable/draw_user_item_sel"
        android:dividerHeight="@dimen/layout_1dp"></ListView>

</com.peoit.android.peoit_lib.view.cardView.LinearCradView>
